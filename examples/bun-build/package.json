{
  "name": "bunbuild",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "prepare": "ts-patch install && typia patch",
    "test": "run-s test:*",
    "test:runtime": "bun run ./index.ts",
    "test:build": "npm run build && bun run ./out/*.js",
    "build": "bun run ./build.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "npm-run-all2": "^6.2.0",
    "typia": "^6.0.6"
  },
  "peerDependencies": {
    "ts-patch": "^3.1.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "unplugin-typia": "workspace:*"
  }
}
